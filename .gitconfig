[credential]
	helper = osxkeychain
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[pull]
    rebase = true
[push]
	default = upstream
[alias]
	st = status
	co = checkout
	cob = checkout -b
	cm = commit -m
	df = diff
	br = branch
	db = branch -D
    rm = rm --cached
	lg = log --oneline --graph
	lgp = log --graph --decorate --pretty=oneline --abbrev-commit
